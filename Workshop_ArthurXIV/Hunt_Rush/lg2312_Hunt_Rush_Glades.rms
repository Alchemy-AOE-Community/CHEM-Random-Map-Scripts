base_layer ICE

create_land {                        land_id 1            terrain_type GRASS_JUNGLE land_percent 99 zone 1 base_elevation 2 land_position 49 49
             if     TINY_MAP     top_border 8 bottom_border 8 left_border 8 right_border 8
             elseif SMALL_MAP    top_border 7 bottom_border 7 left_border 7 right_border 7
             elseif MEDIUM_MAP   top_border 6 bottom_border 6 left_border 6 right_border 6
             elseif LARGE_MAP    top_border 5 bottom_border 5 left_border 5 right_border 5
             elseif HUGE_MAP     top_border 5 bottom_border 5 left_border 5 right_border 5
             elseif GIGANTIC_MAP top_border 4 bottom_border 4 left_border 4 right_border 4
             elseif LUDIKRIS_MAP top_border 4 bottom_border 4 left_border 4 right_border 4
             endif }

if 2_PLAYER_GAME
	start_random
		percent_chance 50 #define ANGLE1
		percent_chance 50 #define ANGLE2
	end_random
	/* land_id should always be placed after assign_to_player, otherwise each player gets several starting TCs etc */
	create_land { assign_to_player 1           base_size 2 terrain_type GRASS_JUNGLE land_percent 0 zone 1 base_elevation 4 top_border 7 bottom_border 7 right_border 7 left_border 7 border_fuzziness 12
	             if     TINY_MAP     circle_radius 22 0
	             elseif SMALL_MAP    circle_radius 23 0
	             elseif MEDIUM_MAP   circle_radius 21 0
	             elseif LARGE_MAP    circle_radius 20 0
	             elseif HUGE_MAP     circle_radius 20 0
	             elseif GIGANTIC_MAP circle_radius 21 0
	             elseif LUDIKRIS_MAP circle_radius 20 0
	             endif }
	create_land { assign_to_player 2 if ANGLE1 else land_id 1 endif
	                      base_size 2 terrain_type GRASS_JUNGLE land_percent 0 zone 1 base_elevation 4 top_border 7 bottom_border 7 right_border 7 left_border 7 border_fuzziness 12 }
	create_land { assign_to_player 2 if ANGLE2 else land_id 1 endif
	                      base_size 2 terrain_type GRASS_JUNGLE land_percent 0 zone 1 base_elevation 4 top_border 7 bottom_border 7 right_border 7 left_border 7 border_fuzziness 12 }
else
	create_player_lands	{ base_size 2 terrain_type GRASS_JUNGLE land_percent 0 zone 1 base_elevation 4 top_border 7 bottom_border 7 right_border 7 left_border 7 border_fuzziness 12
	             if     TINY_MAP     circle_radius 22 0
	             elseif SMALL_MAP    circle_radius 23 0
	             elseif MEDIUM_MAP   circle_radius 21 0
	             elseif LARGE_MAP    circle_radius 20 0
