#const AMAZON_WARRIOR 825
#const AMAZON_ARCHER 850
#const FISH_FOOD rnd(455,457) /* DORADO, SALMON, TUNA  */
#const MULE_CART 1808
#const BANDIT 299 /* melee unit */
