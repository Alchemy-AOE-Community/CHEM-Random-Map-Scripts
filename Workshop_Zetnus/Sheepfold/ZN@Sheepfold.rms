/*
reduced the width of center area so that sheep are never safe
disabled the default 9 vil start, you can still get it by activating empire wars in dark age

*/



/* #define EMPIRE_WARS */	/* used for the 9 vill start */
#define TRUE	/* used to collapse long code */

/* #define ACTIVATE_TREE_OUTLINES */

/* terrains */
#const OAK_BUSH_FOREST 20
#const FARM_DEAD 8
#const FARM_33% 30
#const GRAVEL_DEFAULT 70
#const UNDERBURSH 71
#const WET_GRAVEL 81
#const WET_ROCK 82
#const BUSH_FOREST 89

/* objects */

#const SHEEP_DEAD 595
#const FALCON 1056
#const ROCK2 1323

#const HERDABLE_A 1245 /* Pig */
#const HERDABLE_B 1060 /* Goat */
#const S_STRAGGLER 1250 /* dead tree */

#const ONGRID_PLACEHOLDER 278	/* dead fish trap */
#const PH_NEUTRAL_OFF 346 /* Off-Grid Object - only used for gaia objects */
#const PH_PLAYER_OFF 647 /* Off-Grid Object - only used for player objects */
#const BLANKOBJECT 647

#const LAZY_VILLAGER 1285 /* FE Flag */
#const LUMBERJACK_FEMALE 218
#const LUMBERJACK_MALE 123

#const EW_REVEALER 851 /* ES Flag */

/* =================================================================================================== */
<PLAYER_SETUP>

random_placement
behavior_version 2

effect_amount SET_ATTRIBUTE SHEEP ATTR_HITPOINTS 100
effect_amount SET_ATTRIBUTE SHEEP ATTR_LINE_OF_SIGHT 12
effect_amount SET_ATTRIBUTE SHEEP_DEAD ATTR_DEAD_ID 594
effect_amount SET_ATTRIBUTE SHEEP ATTR_BASE_ARMOR 99
effect_amount SET_ATTRIBUTE SHEEP ATTR_STORAGE_VALUE 0
effect_amount SET_ATTRIBUTE SHEEP ATTR_HIDDEN_DAMAGE_RESIST 1
effect_amount SET_ATTRIBUTE SHEEP ATTR_HERO_STATUS 72

effect_amount SET_ATTRIBUTE ROCK2 ATTR_OCCLUSION_MODE 2
if ACTIVATE_TREE_OUTLINES
effect_amount SET_ATTRIBUTE DLC_AFRICANBUSH_2 ATTR_OCCLUSION_MODE 5
endif

effect_amount SET_ATTRIBUTE LAZY_VILLAGER ATTR_DEAD_ID 83
effect_amount SET_ATTRIBUTE LAZY_VILLAGER ATTR_HITPOINTS 0

if EMPIRE_WARS
	if DARK_AGE_START
		#define EW_DARK
		effect_amount MOD_RESOURCE AMOUNT_STARTING_WOOD ATTR_ADD -30
		effect_amount MOD_RESOURCE AMOUNT_STARTING_FOOD ATTR_ADD -100
		effect_amount GAIA_ADD_ATTRIBUTE HERDABLE_A ATTR_STORAGE_VALUE -30	
		effect_amount GAIA_SET_ATTRIBUTE HERDABLE_A ATTR_HITPOINTS 0
		effect_amount SET_ATTRIBUTE EW_REVEALER ATTR_LINE_OF_SIGHT 18
		effect_amount SET_ATTRIBUTE EW_REVEALER ATTR_HITPOINTS 0
	elseif FEUDAL_AGE_START
		#define EW_FEUDAL
	elseif CASTLE_AGE_START
		#define EW_CASTLE
	elseif IMPERIAL_AGE_START
		#define EW_IMPERIAL
	elseif POST_IMPERIAL_AGE_START
		#define EW_POST
	else
		#define EW_DEFAULT
	endif
endif

/* Disabling Trade Carts and Cogs for Multi-team Games */
if 2_TEAM_GAME
else
  #const TRADE_CART_ENABLER 161
  #const TRADE_COG_ENABLER 180
  #const TRADE_CARAVAN_ENABLER 48
  #const HINDU_CARAVANSERAI_ENABLER 518
  #const PERSIAN_CARAVANSERAI_ENABLER 552  
  #const SILK_ROAD 499
  effect_amount DISABLE_TECH TRADE_COG_ENABLER ATTR_DISABLE 180
  effect_amount DISABLE_TECH TRADE_CART_ENABLER ATTR_DISABLE 161
  effect_amount DISABLE_TECH TRADE_CARAVAN_ENABLER ATTR_DISABLE 48
  effect_amount DISABLE_TECH HINDU_CARAVANSERAI_ENABLER ATTR_DISABLE 518
  effect_amount DISABLE_TECH PERSIAN_CARAVANSERAI_ENABLER ATTR_DISABLE 552
  effect_amount DISABLE_TECH SILK_ROAD ATTR_DISABLE 499    
endif

/* =================================================================================================== */
<LAND_GENERATION>

base_terrain WET_ROCK

create_player_lands {
	terrain_type DLC_ROCK
	base_size 8
	land_percent 90
	set_zone_by_team
	other_zone_avoidance_distance 6
	circle_radius 15 0
	left_border 15 right_border 15 top_border 15 bottom_border 15
	border_fuzziness 100
	base_elevation 3
}


